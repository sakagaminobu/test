<table>
    <tr>
        <th>id</th>
        <th>name</th>
        <th>email</th>
        <th>password</th>
    </tr>
    <% users.forEach((user) => { %>
    <tr>
      <form action="/removeUser/<%= user.id %>" method="post">
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.password %></td>
        <td><input type="submit" value="削除"></td>
      </form>
    </tr>
    <% }) %>
</table>
<a href="/host">管理者ページに戻る</a>